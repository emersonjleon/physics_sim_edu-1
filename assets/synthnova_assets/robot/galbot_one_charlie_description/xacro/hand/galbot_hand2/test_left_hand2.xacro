<robot name="test_hand" xmlns:xacro="http://ros.org/wiki/xacro">
    <!-- Include-->
    <xacro:include filename="$(find galbot_one_charlie_description)/xacro/hand/galbot_hand2/galbot_left_hand2.xacro" />
    <!-- Properties -->
    <xacro:property name="M_PI" value="3.1415926535897931" />
    <!-- Parent link -->
    <link name="base_link" />
    <xacro:galbot_left_hand2 parent="base_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:galbot_left_hand2>
</robot>