<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="right_arm">


  <xacro:macro name="inertial_matrix" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0" />
    </inertial>
  </xacro:macro>
  <xacro:macro name="Box_inertial_matrix" params="m l w h">
    <inertial>
      <mass value="${m}" />
      <inertia ixx="${m*(h*h + l*l)/12}" ixy="0" ixz="0" iyy="${m*(w*w + l*l)/12}" iyz="0"
        izz="${m*(w*w + h*h)/12}" />
    </inertial>
  </xacro:macro>


  <xacro:macro name="right_arm" params="parent *origin name:=arm">
    <xacro:property name="M_PI" value="3.1415926535897931" />

    <joint name="${name}_joint" type="fixed">
      <xacro:insert_block name="origin" />
      <parent link="${parent}" />
      <child link="${name}_base_link" />
    </joint>

    <link name="${name}_base_link">
      <inertial>
        <origin rpy="0 0 0" xyz="-0.00037 -0.00006 -0.11407" />
        <mass value="0.9" />
        <inertia ixx="0.01248" ixy="0.00000" ixz="0.00003" iyy="0.01247" iyz="0.00003" izz="0.00056" />
      </inertial>
    </link>
    <link name="${name}_link1">
      <inertial>
        <origin rpy="0 0 0" xyz="0.00000 -0.00650 -0.01459" />
        <mass value="1.48" />
        <inertia ixx="0.00378" ixy="0.00000" ixz="0.00000" iyy="0.00460" iyz="0.00008" izz="0.00217" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link1.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link1.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint1" type="revolute">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${name}_base_link" />
      <child link="${name}_link1" />
      <axis xyz="0 0 1" />
      <limit lower="-3" upper="3" effort="60" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link2">
      <inertial>
        <origin rpy="0 0 0" xyz="-0.00080 -0.09873 0.00815" />
        <mass value="1.33" />
        <inertia ixx="0.01827" ixy="0.00003" ixz="0.00001" iyy="0.00232" iyz="0.00080" izz="0.01769" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link2.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link2.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint2" type="revolute">
      <origin xyz="0 0 0" rpy="${-M_PI/2} 0 ${M_PI}" />
      <parent link="${name}_link1" />
      <child link="${name}_link2" />
      <axis xyz="0 0 1" />
      <limit lower="-${M_PI/2}" upper="1.7" effort="60" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link3">
      <inertial>
        <origin rpy="0 0 0" xyz="-0.00019 -0.00330 -0.04824" />
        <mass value="1.25" />
        <inertia ixx="0.00955" ixy="0.00000" ixz="0.00001" iyy="0.00983" iyz="0.00007" izz="0.00151" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link3.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link3.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint3" type="revolute">
      <origin xyz="0 -0.35 0" rpy="${M_PI/2} ${-M_PI/2} 0" />
      <parent link="${name}_link2" />
      <child link="${name}_link3" />
      <axis xyz="0 0 1" />
      <limit lower="-2.9" upper="2.9" effort="30" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link4">
      <inertial>
        <origin rpy="0 0 0" xyz="0.00000 -0.06457 0.00899" />
        <mass value="0.8" />
        <inertia ixx="0.00529" ixy="0.00000" ixz="0.00000" iyy="0.00120" iyz="0.00030" izz="0.00488" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link4.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link4.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint4" type="revolute">
      <origin xyz="0 0 0" rpy="${-M_PI/2} 0 ${M_PI}" />
      <parent link="${name}_link3" />
      <child link="${name}_link4" />
      <axis xyz="0 0 1" />
      <limit lower="-2.1" upper="2.1" effort="30" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link5">
      <inertial>
        <origin rpy="0 0 0" xyz="0.00201 -0.00042 -0.14589" />
        <mass value="1.22" />
        <inertia ixx="0.03428" ixy="0.00001" ixz="0.00032" iyy="0.03366" iyz="0.00005" izz="0.00222" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link5.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link5.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint5" type="revolute">
      <origin xyz="0 -0.36 0" rpy="${M_PI/2} 0 0" />
      <parent link="${name}_link4" />
      <child link="${name}_link5" />
      <axis xyz="0 0 1" />
      <limit lower="-2.9" upper="2.9" effort="10" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link6">
      <inertial>
        <origin rpy="0 0 0" xyz="0.00000 0.01031 0.00000" />
        <mass value="0.43" />
        <inertia ixx="0.00051" ixy="0.00000" ixz="0.00000" iyy="0.00070" iyz="0.00000" izz="0.00091" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link6.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link6.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint6" type="revolute">
      <origin xyz="0 0 0" rpy="${-M_PI/2} ${M_PI/2} ${M_PI}" />
      <parent link="${name}_link5" />
      <child link="${name}_link6" />
      <axis xyz="0 0 1" />
      <limit lower="-0.785398163" upper="0.785398163" effort="10" velocity="1.5" acceleration="2"
        jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>
    <link name="${name}_link7">
      <inertial>
        <origin rpy="0 0 0" xyz="-0.03291 0.00799 -0.00131" />
        <mass value="0.6" />
        <inertia ixx="0.00124" ixy="0.00030" ixz="0.00013" iyy="0.00198" iyz="0.00007" izz="0.00164" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link7.obj" />
        </geometry>

      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://galbot_one_charlie_description/meshes/arm/right_arm_link7.stl" />
        </geometry>
      </collision>
    </link>
    <joint name="${name}_joint7" type="revolute">
      <origin xyz="0 0 0" rpy="${-M_PI/2} 0 0" />
      <parent link="${name}_link6" />
      <child link="${name}_link7" />
      <axis xyz="0 0 1" />
      <limit lower="-1.4" upper="1.4" effort="10" velocity="1.5" acceleration="2" jerk="12" />
      <dynamics damping="0" friction="0" />
    </joint>

    <link name="${name}_end_effector_mount_link">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="0" />
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      </inertial>
    </link>
    <joint name="${name}_end_effector_mount_joint" type="fixed">
      <origin xyz="-0.083 0 0" rpy="${M_PI/2} 0 ${M_PI}" />
      <parent link="${name}_link7" />
      <child link="${name}_end_effector_mount_link" />
      <axis xyz="0 0 0" />
    </joint>


  </xacro:macro>

</robot>